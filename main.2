#include <iostream>
#include <cmath>

struct Point {
  double x;
  double y;
};

double distanceBetweenPoints(const Point& p1, const Point& p2) {
  double dx = p2.x - p1.x;
  double dy = p2.y - p1.y;
  return std::sqrt(dx * dx + dy * dy);
}

int main() {
  Point point1;
  point1.x = 1.0;
  point1.y = 2.0;

  Point point2;
  point2.x = 4.0;
  point2.y = 6.0;

  double distance = distanceBetweenPoints(point1, point2);

  std::cout << "Відстань між точками: " << distance << std::endl;

  return 0;
}
